#include<stdio.h>
#include<stdlib.h>
struct node{
	int data;
	struct node* next;
};

struct node* addatbeg(struct node* start){
	struct node* temp;
	int val;
	printf("Enter the value : ");
	scanf("%d",&val);
	temp=(struct node*)malloc(sizeof(start));
	temp->data=val;
	start->next=temp;
	start=temp;
	return start;
}
void printlist(struct node* start){
	struct node* p;
	p=start;
	while(p!=NULL){
		printf("%d",p->data);
		p=p->next;
	}
}


int main(){
	struct node *start=NULL;
	start=addatbeg(start);
	printlist(start);
	
	
	return 0;
}
